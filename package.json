{
  "name": "verdaccio-cdk-ec2",
  "version": "1.0.0",
  "description": "Provisions EC2 instance and runs private Verdaccio registry via CDK.",
  "author": "Andrew Kirchmyer",
  "license": "Apache 2.0",
  "keywords": [
    "AWS",
    "CDK",
    "ec2",
    "Verdaccio"
  ],
  "bin": {
    "ec2-cdk": "bin/ec2-cdk.js"
  },
  "bugs": {
    "url": "https://github.com/akirchmyer/verdaccio-cdk-ec2/issues"
  },
  "homepage": "https://github.com/akirchmyer/verdaccio-cdk-ec2",
  "scripts": {
    "build": "tsc",
    "cdk": "cdk",
    "format": "prettier --write \"./bin/**/*.+(js|ts|json)\"",
    "lint": "eslint ./bin --ext .ts",
    "lint:fix": "eslint ./bin --ext .ts --fix && npm run format",
    "watch": "tsc -w"
  },
  "devDependencies": {
    "@types/node": "18.7.2",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "aws-cdk": "*",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.7.1",
    "typescript": "~4.7.4"
  },
  "dependencies": {
    "aws-cdk-lib": "2.37.1",
    "cdk-ec2-key-pair": "^3.3.1",
    "constructs": "^10.0.0",
    "simple-cdk-ec2": "^1.0.2",
    "source-map-support": "^0.5.16"
  }
}
